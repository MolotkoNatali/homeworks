# Test source

```ts
   1 | import { Locator, Page } from '@playwright/test';
   2 |
   3 | export class FutunaturaPage {
   4 |     private get searchInput(): Locator {
   5 |         return this.page.locator('input[type="search"][name="search"]');
   6 |     }
   7 |
   8 |     private get addToCartButton(): Locator {
   9 |         return this.page.locator('#button-cart.add_btn');
  10 |     }
  11 |
  12 |     private get firstProduct(): Locator {
  13 |         return this.page.locator('#main1 > div > div:nth-child(1)');
  14 |     }
  15 |
  16 |     private get banner(): Locator {
  17 |         return this.page.locator('img[src*="banner"]');
  18 |     }
  19 |
  20 |     private get bestSellerSection(): Locator {
  21 |         return this.page.locator('#embla-best-seller');
  22 |     }
  23 |
  24 |     public constructor(private page: Page) {}
  25 |
  26 |     public async waitForPageReady(): Promise<void> {
  27 |         await this.page.waitForLoadState('load');
> 28 |         await this.page.waitForSelector('body');
     |                         ^ Error: page.waitForSelector: Test timeout of 30000ms exceeded.
  29 |     }
  30 |
  31 |     public getSearchInput(): Locator {
  32 |         return this.searchInput;
  33 |     }
  34 |
  35 |     public async getAddToCartButton(): Promise<Locator> {
  36 |         const addToCartButton = this.page.locator('#button-cart.add_btn');
  37 |         await addToCartButton.waitFor({ state: 'visible', timeout: 5000 });
  38 |         return addToCartButton;
  39 |     }
  40 |
  41 |     public async waitForAddToCartButton(): Promise<void> {
  42 |         await this.addToCartButton.waitFor({ state: 'visible', timeout: 5000 });
  43 |     }
  44 |
  45 |     public async getFirstProduct(): Promise<Locator> {
  46 |         const firstProduct = this.firstProduct;
  47 |         await firstProduct.waitFor({ state: 'visible', timeout: 5000 });
  48 |         return firstProduct;
  49 |     }
  50 |
  51 |     public getBanner(): Locator {
  52 |         return this.banner;
  53 |     }
  54 |
  55 |     public getBestSellerSection(): Locator {
  56 |         return this.bestSellerSection;
  57 |     }
  58 |
  59 |     public getPage(): Page {
  60 |         return this.page;
  61 |     }
  62 |
  63 |     public async goTo(): Promise<void> {
  64 |         await this.page.goto('https://www.futunatura.si/');
  65 |         await this.waitForPageReady();
  66 |     }
  67 |
  68 |     public async searchForTerm(searchTerm: string): Promise<void> {
  69 |         const searchField = this.getSearchInput();
  70 |         await searchField.fill(searchTerm);
  71 |         await searchField.press('Enter');
  72 |     }
  73 |
  74 |     public async expectSearchResultsToBeVisible(): Promise<void> {
  75 |         const itemsContainer = this.page.locator('div.row.gx-3');
  76 |         await itemsContainer.waitFor({ state: 'visible' });
  77 |     }
  78 |
  79 |     public async addProductToCart(): Promise<void> {
  80 |         await this.addToCartButton.click();
  81 |     }
  82 |
  83 |     public async selectCategory(): Promise<void> {
  84 |         await this.page.locator('.menu_wrapper ul > li:nth-child(3)').click();
  85 |     }
  86 |
  87 |     public async clickBanner(): Promise<void> {
  88 |         await this.page.locator('div.main-banner-btn:has-text("Na ponudbo")').click();
  89 |     }
  90 | }
